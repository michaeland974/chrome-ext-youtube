{
    "name": "Minimize for Youtube",
    "description": "Simplifying the Youtube Homepage",
    "version": "1.0",
    "manifest_version": 3,
    "action": {},
    "permissions": [
       "activeTab",
       "scripting",
       "tabs",
       "webNavigation"
    ],
    "background": {
       "service_worker": "background.js",
       "type": "module"
    },
    "host_permissions": [
        "https://www.youtube.com/"
    ]
 }